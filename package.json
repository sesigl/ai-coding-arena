{
  "name": "ai-coding-arena",
  "version": "1.0.0",
  "type": "module",
  "description": "LLM coding competition system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "CI=true vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:integration": "DEBUG=true ENABLE_CLAUDE_INTEGRATION_TESTS=true vitest --run src/providers/claude-code-provider/claude-code-provider.integration.test.ts",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run typecheck && npm run lint && npm run format:check && npm test",
    "fix:all": "npm run lint:fix && npm run format && npm test",
    "cli": "tsx src/cli/index.ts",
    "prepare": "husky"
  },
  "keywords": [
    "llm",
    "competition",
    "coding",
    "typescript"
  ],
  "author": "Sebastian Sigl",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "1.0.56",
    "dotenv": "17.2.1",
    "duckdb": "1.3.2",
    "neverthrow": "8.0.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "24.0.15",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.32.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "^3.4.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
